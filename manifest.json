{
    "manifest_version": 2,
    "name": "sample app",
    "version": "0.0.1",
    "description": "sample description",
    "icons": {
        "16": "images/icon/16.png",
        "19": "images/icon/19.png",
        "32": "images/icon/32.png",
        "38": "images/icon/38.png",
        "48": "images/icon/48.png",
        "128": "images/icon/128.png"
    },
    "permissions": [
        "tabs"
    ],
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "script.js"
            ]
        }
    ]
}